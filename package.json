{
  "name": "prayerswebapp",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "tsc",
    "bundlejs": "browserify lib/public/js/settings.js -o lib/public/js/bundle.js --debug",
    "roolupjs": "rollup lib/public/js/settings.js --format iife --name \"main\"  --file lib/public/js/bundle.js",
    "html:copy": "copyfiles -u 1 src/**.html lib",
    "css:copy": "copyfiles -u 1 src/css/**.css lib/",
    "images:copy": "copyfiles -u 1 src/images/**/* lib/",
    "start:server": "node lib/index.js",
    "reload": "browser-sync reload --port 7000",
    "start": "npm run compile  && npm run bundlejs &&  npm run reload && npm run start:server "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DPANET/prayersWebApp.git"
  },
