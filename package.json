  "bugs": {
    "url": "https://github.com/DPANET/prayersWebApp/issues"
  },
  "homepage": "https://github.com/DPANET/prayersWebApp#readme",
  "description": "",
  "dependencies": {
    "@dpanet/prayers-lib": "^1.0.50",
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.34",
    "await-to-js": "^2.1.1",
    "body-parser": "^1.19.0",
    "config": "^3.1.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jquery": "^3.4.0",
    "lowdb": "^1.0.0",
    "morgan": "^1.9.1",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@types/browserify": "^12.0.36",
    "@types/joi": "^14.3.3",
    "@types/jquery": "^3.3.29",
    "@types/lodash": "^4.14.123",
    "@types/lowdb": "^1.0.7",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.13.8",
    "@types/ramda": "^0.26.8",
    "@types/request": "^2.48.1",
    "@types/request-promise-native": "^1.0.16",
    "@types/typescript": "^2.0.0",
    "browser-sync": "^2.26.5",
    "gulp": "^4.0.1",
    "gulp-sass": "^4.0.2",
    "typescript": "^3.4.5"
  }
