    "config": "^3.1.0",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "noty": "^3.2.0-beta"
  },
  "devDependencies": {
    "@dpanet/prayers-lib": "^1.3.9",
    "@sentry/node": "^5.4.0",
    "@types/async-retry": "^1.4.1",
    "@types/body-parser": "^1.17.0",
    "@types/browserify": "^12.0.36",
    "@types/config": "0.0.34",
    "@types/datatables.net": "^1.10.17",
    "@types/daterangepicker": "^3.0.1",
    "@types/debug": "^4.1.4",
    "@types/hapi__joi": "^15.0.1",
    "@types/jquery": "^3.3.29",
